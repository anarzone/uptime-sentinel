{
    "dev": {
        "baseUrl": "https://uptime-sentinel.test",
        "contentType": "application/json",
        "variables": {
            "monitorId": "{{monitorId}}",
            "alertRuleId": "{{alertRuleId}}",
            "escalationPolicyId": "{{policyId}}"
        },
        "defaults": {
            "requestTimeout": "30000",
            "followRedirects": true,
            "validateCertificates": false
        },
        "notes": [
            "Update baseUrl if your Symfony server runs on a different port",
            "Replace variable placeholders with actual IDs from API responses",
            "Use XDEBUG_SESSION_START=1 query param for debugging in PhpStorm"
        ]
    }
}
